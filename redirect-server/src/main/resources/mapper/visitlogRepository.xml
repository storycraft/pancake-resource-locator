<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sh.pancake.link.redirection.repository.VisitlogRepository">
    <insert id="addVisitlog" parameterType="sh.pancake.link.redirection.repository.visitlog.Visitlog">
        INSERT INTO visitlog
            (redirection_id, visitor_address, visited_at)
        VALUES (#{redirectionId}, #{visitorAddress}, #{visitedAt});
    </insert>
</mapper>
